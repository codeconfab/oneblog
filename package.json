    "date-fns": "2.9.0",
    "email-reply-parser": "^1.2.1",
    "feed": "^4.0.0",
    "gif-stream": "^1.1.0",
    "grommet": "2.15.0",
    "grommet-icons": "^4.7.0",
    "grommet-theme-hpe": "^2.3.0",
    "inline-css": "^2.5.1",
    "intersection-observer": "^0.11.0",
    "lower-case": "^2.0.1",
    "neuquant": "^1.0.2",
    "next": "^12.0.0",
    "node-fetch": "^2.6.0",
    "onegraph-auth": "^3.0.0",
    "pixel-stream": "^1.0.3",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "react-intersection-observer": "^8.28.5",
    "react-markdown": "^4.3.1",
    "react-player": "^2.6.2",
    "react-relay": "0.0.0-experimental-183bdd28",
    "relay-runtime": "^10.0.1",
    "scriptjs": "^2.5.9",
    "sentence-case": "^3.0.3",
    "sitemap": "^6.3.2",
    "styled-components": "^5.0.0"
  },
  "scripts": {
    "dev": "next",
    "start": "next",
    "build": "yarn ensure-publish-label && yarn relay && next build",
    "start:prod": "NODE_ENV=production node build/server.js",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --persistFunction scripts/persistQuery.js",
    "relay:clean": "rm -r src/__generated__",
    "flow": "flow",
    "fetch-schema": "node scripts/fetchSchema.js --path schema.graphql",
    "ensure-publish-label": "node scripts/ensurePublishLabel.js",
    "deploy": "vercel",
    "prettier": "prettier --write \"src/**/*.js\" README.md"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-flow": "^7.0.0",
    "@types/react": "^17.0.38",
    "babel-eslint": "10.x",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-relay": "^8.0.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "12.0.9",
    "flow-typed": "^3.2.1",
    "graphql": "^14.4.2",
    "prettier": "^2.1.2",
    "razzle": "^3.0.0",
    "relay-compiler": "^8.0.0",
    "relay-test-utils": "^8.0.0",
    "require-times": "^1.1.0",
    "source-map-explorer": "^2.0.1",
    "typescript": "^4.5.5"
  },
  "babelMacros": {
    "relay": {}
  }
}
